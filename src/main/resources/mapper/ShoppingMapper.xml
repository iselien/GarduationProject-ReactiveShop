<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hopu.mapper.ShoppingMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hopu.entity.Shopping">
        <id column="shopping_id" property="shoppingId" />
        <result column="users_id" property="usersId" />
        <result column="goods_id" property="goodsId" />
        <result column="shoppingnum" property="shoppingnum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        shopping_id, users_id, goods_id, shoppingnum
    </sql>

</mapper>
